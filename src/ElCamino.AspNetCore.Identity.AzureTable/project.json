{
    "version": "1.0.0",
    "name": "ElCamino.AspNetCore.Identity.AzureTable",
    "title": "Azure Table Storage Provider for ASP.NET Identity Core",
    "description": "Azure Table Storage Provider to support AspNet Identity Core 1.0 frameworks for .NET Standard and Core.",
    "authors": [ "David Melendez" ],
    "copyright": "Copyright Â©  2016 David Melendez, MIT License",
    "packOptions": {
        "tags": [ "ASP.NET", "Identity Core", "Azure", "Table Storage", "MVC", "UserStore", "RoleStore", "UserManager", "multi-tenant", "RA-GRS", "geo-redundant", "Microsoft.AspNetCore.Identity" ],
        "requireLicenseAcceptance": true,
        "owners": [ "David Melendez" ],
        "licenseUrl": "https://github.com/dlmelendez/identityazuretable/blob/master/LICENSE",
        "releaseNotes": "Initial release to support AspNetCore.Identity 1.0 frameworks for .NET Standard and Core.",
        "repository": {
            "type": "git",
            "url": "https://github.com/dlmelendez/identityazuretable.git"
        },
        "summary": "Azure Table Storage Provider to support AspNet Identity Core 1.0"
    },

    "buildOptions": {
        "keyFile": "../../tools/Key.snk"
    },
    "dependencies": {
        "NETStandard.Library": "1.6.0",
        "Microsoft.AspNetCore.Identity": "1.0.0",
        "Microsoft.Extensions.DependencyInjection": "1.0.0",
        "Newtonsoft.Json": "9.0.1",
        "WindowsAzure.Storage": "7.1.3-preview"
    },

    "frameworks": {
        "net451": {
            "buildOptions": { "define": [ "net451" ] }
        },
        "netstandard1.3": {
            "imports": [
                "dotnet5.6",
                "dnxcore50",
                "portable-net451+win8"
            ],
            "buildOptions": { "define": [ "netstandard" ] }

        }
    }
}